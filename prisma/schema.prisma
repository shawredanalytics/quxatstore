generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/client"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Document {
  id          Int      @id @default(autoincrement())
  title       String
  description String?
  fileUrl     String
  fileName    String
  fileType    String?
  category    String?
  uploadedAt  DateTime @default(now())
}
